Option Public
Option Declare
Use "libLog"
Private gl_isInitialyzed As String
Sub Initialize
	Call libLogConfig_init()
End Sub
Sub libLogConfig_init()
	If "yes" = gl_isInitialyzed Then
		Exit Sub
	End If
	
	Call setLogLevel( "ALL", LL_DEBUG )
	Call setLogLevel( "libT", LL_INFO )
	'Call setLogLevel( "libT||ttt", LL_DEBUG )
	Call setLogLevel( "agttt", LL_ERROR )
	Call setLogLevel( "agttt|Mc", LL_WARN )
	Call setLogLevel( "agttt|Mc|testMe", LL_DEBUG )
	Call setLogLevel( "ALL", LL_NONE )
	'Call setLogLevel( "ALL", LL_WARN )
	'Call setLogLevel( "ALL", LL_ALL )
	Call setLogLevel( "ALL", LL_NONE )
	Call setLogLevel( "agttt||ATTT", LL_ALL )
	Call setLogLevel( "libTracer", LL_INFO )
	Call setLogLevel( "libTracer", LL_DEBUG )
	Call setLogLevel( "ALL", LL_WARN )
	
	gl_isInitialyzed  = "yes"
End Sub
